@incollection{lavengood_timbre_2019,
	address = {London},
	title = {Timbre, {Genre}, and {Polystylism} in \textit{{Sonic} the {Hedgehog} 3}},
	copyright = {All rights reserved},
	isbn = {978-3-030-18098-0},
	url = {https://link.springer.com/chapter/10.1007/978-3-030-18099-7_11},
	abstract = {In the soundtrack for the Sega Genesis game Sonic the Hedgehog 3 (1994), the genres represented include calypso, funk, carnival, new wave, prog rock, and more. Soundtracks for video games frequently shift genres this way, to create aesthetic themes for different levels or characters. Turning toward an account of the game’s soundtrack as a unified and continuous work, I posit that the music of Sonic the Hedgehog 3 might be understood as analogous to a series of “samples” within a polystylistic whole, following Leydon 2010. Leydon notes that instrumentation “bears the bulk of the semiotic burden” in communicating genre, but stops short of detailing how different instrumental timbres themselves might signify these genres. In my close analysis of two specific levels from Sonic the Hedgehog 3—Ice Cap Zone and Marble Garden Zone—I detail how timbre, as a musical parameter separate from instrumentation, can evoke specific intertextual and “extra”-musical associations from a listener, based on implied genres in the soundtrack. In doing this, I will show how timbre, a musical parameter that remains overlooked in a great deal of music analysis, might inform and enhance dialogue in music analyses of genre within video game music and more broadly.},
	language = {en},
	urldate = {2019-12-06},
	booktitle = {On {Popular} {Music} and {Its} {Unruly} {Entanglements}},
	publisher = {Palgrave Macmillan},
	author = {Lavengood, Megan L.},
	editor = {Braae, Nick and Hansen, Kai Arne},
	year = {2019},
	doi = {10.1007/978-3-030-18099-7},
	pages = {209--234},
	file = {Lavengood - 2019 - Timbre, Genre, and Polystylism in iSonic the Hedgehog 3-i.pdf:/Users/megan/Documents/Academic readings/Lavengood - 2019 - Timbre, Genre, and Polystylism in iSonic the Hedgehog 3-i.pdf:application/pdf;Snapshot:/Users/megan/Zotero/storage/8GQ4VVCI/9783030180980.html:text/html},
